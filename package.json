			}
		]
	},
	"scripts": {
		"version": "vsce publish",
		"postversion": "git push --tags --quiet origin master:master"
	},
	"devDependencies": {
		"vscode": "^1.1.0"
	},
	"dependencies": {
		"babylon": "6.16.1",
		"glob": "7.1.1",
		"lodash": "4.17.4"
	}
